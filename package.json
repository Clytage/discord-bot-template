{
  "name": "cyteliz-discord-bot-template-djs",
  "version": "1.0.0",
  "private": true,
  "description": "Cyteliz Discord Bot Template.",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run lint && npm run compile",
    "lint": "eslint . --ext .ts",
    "compile": "tsc --build tsconfig.json",
    "start": "node --es-module-specifier-resolution=node -r dotenv/config ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyteliz/discord-bot-template.git"
  },
  "author": "Mednoob <ahmadyaqdhan123@gmail.com>",
  "license": "AGPL-3.0",
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "@cyteliz/eslint-config/typescript"
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "got": "^12.0.4",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@cyteliz/eslint-config": "^1.0.0",
    "@types/node": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "typescript": "^4.6.3"
  }
}
